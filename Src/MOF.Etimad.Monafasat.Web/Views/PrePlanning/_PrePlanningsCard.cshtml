<div class="animated  delay-2s col-md-12 col-sm-12 col-xs-12" id="PrePlanningsTable">
   <div class="row" v-if="PrePlannings && PrePlannings.length > 0">
      <div class="col-xl-4 col-lg-4 col-md-6  col-sm-12 col-xs-12" v-for="(obj, $index) in PrePlannings">
         <div class="card">
            <div class="card-body">
               @Html.Partial("_PrePlanningListActions.cshtml")
               <h5 class="card-category card-category-social text-primary">
                  <i class="material-icons  text-dark-green" style="border: 1px solid #ccc;display: inline;border-radius: 100%;padding: 5px;">people</i> <span class="d-inline-block" style="padding: 0 5px;"> {{obj.projectName}} </span>

               </h5>
               <div class="clearfix"></div>
               <div class="row">
                  <ul class="list-group form-details-list">
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              #
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{(currentPage -1) * (pageSize) +  $index + 1}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.TenderResources.DisplayInputs.Agencies
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{obj.agencyName}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.YearQuarter
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{obj.yearQuarterName}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              السنه
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{obj.year}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.ExecutionPlace
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{obj.insideKSAString}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.ProjectType
                           </div>
                           <div class="col-8 etd-item-info">
                              <ol  v-for="(objProjectTypesList, $index) in obj.projectTypesList">
                                 <li>{{objProjectTypesList.text}}</li>
                              </ol>

                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.ProjectNature
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>
                                 {{obj.projectNature}}
                              </span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.ProjectDescription
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>
                                 {{obj.projectDescription}}
                              </span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.Status
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>
                                 {{obj.statusName}} -
                              </span>
                              @if (User.IsInRole(RoleNames.PrePlanningCreator) || User.IsInRole(RoleNames.PrePlanningAuditor))
                              {
                                 <span class="badge badge-warning" v-if="obj.isDeleteRequested==true">
                                    بانتظار اعتماد طلب الحذف
                                 </span>
                                 <span class="badge badge-danger" v-if="obj.isDeleteRequested==false&&obj.deleteRejectionReason!=null">
                                    تم رفض طلب الحذف
                                 </span>
                              }
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.PrePlanningResources.DisplayInputs.Duration
                           </div>
                           <div class="col-8 etd-item-info">
                              <span v-if="obj.durationInDays!=null && obj.durationInMonths !=null && obj.durationInYears!=null ">
                                 {{obj.durationInDays}} يوم - {{obj.durationInMonths}} شهر - {{obj.durationInYears}} سنة
                              </span>
                           </div>
                        </div>
                     </li>
                  </ul>
               </div>
            </div>
         </div>
      </div>
   </div>
   <div class="col-12" v-else>
      <div class="row text-center">
         <div class="card">
            <div class="card-body text-center">
               <i class="text-primary material-icons" style="font-size:3rem">report</i>
               <br />
               @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.NoData
            </div>
         </div>
      </div>
   </div>

</div>
