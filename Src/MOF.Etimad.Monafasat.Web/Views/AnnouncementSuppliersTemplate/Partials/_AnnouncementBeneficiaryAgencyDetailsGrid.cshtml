
         <div class="table-responsive" style="min-height:100px">
            <table  class="table table-striped text-center" summary="desc">
               <thead class="text-center">
                  <tr>
                     <th scope="col" class="text-center">#</th>
                     <th scope="col" class="text-center">@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.AgencyName)</th>
                     <th scope="col" class="text-center">@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.BeneficiaryAgencyStatus)</th>
                     <th scope="col" class="text-center">@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.CreatedDate)</th>
                     <th scope="col" class="text-center">@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.RemovedDate)</th>
                  </tr>
               </thead>
               <tbody class="text-center">

               <tbody class="text-center" v-if="beneficiaryAgency && beneficiaryAgency.length > 0">
                  <tr v-for="(announcementBeneficiaryAgency, $index) in beneficiaryAgency">
                     <td class="text-center" data-label="#">{{ (currentPage -1) * (pageSize) +  $index + 1}}</td>
                     <td class="text-center" data-label="@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.AgencyName)"> {{announcementBeneficiaryAgency.agencyName}} </td>
                     <td class="text-center" data-label="@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.BeneficiaryAgencyStatus)"> {{announcementBeneficiaryAgency.statusName}} </td>
                     <td class="text-center" data-label="@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.CreatedDate)"> {{announcementBeneficiaryAgency.createdDate}} </td>
                     <td class="text-center" data-label="@Html.Raw(MOF.Etimad.Monafasat.Resources.AnnouncementSupplierTemplateResources.DisplayInputs.CreatedDate)"> {{announcementBeneficiaryAgency.removedDate}} </td>
                  </tr>
               </tbody>
               <tbody v-else>
                  <tr>
                     <td colspan="5">
                        <div class=" text-center">
                           <i class="text-primary material-icons" style="font-size:3rem">report</i>
                           <br />
                           @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.NoData
                        </div>

                     </td>
                  </tr>
               </tbody>
            </table>
            <pagination :visible_pages="3" ref="vpaginator" :resource_url="resource_url" @@update="updateResource"></pagination>
         </div>
