<div id="SupplierGrid" v-cloak>
   <div class="card-body">
      <h4 class="text-primary">@MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.SupplierSummary<a data-toggle="collapse" href="#SupplierResult" role="button" aria-expanded="false" aria-controls="SupplierResult" class="btn p-2 btn-outline-primary pull-right"><i class="material-icons">search</i> @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.Search</a></h4>
      <div class="clearfix"></div>
      <div class=" collapse" id="SupplierResult">
         <div class="row">
            <div class="col-md-6 col-sm-6 col-xs-12">
               <div class="form-group">
                  <label id="duration"  for="publisher" class="label bmd-label-static" title="--أختر الفترة--" data-style="select-with-transition"> @MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.TimePeriod<span class="text-danger">*</span></label>
                  <select data-live-search="false"  name="Duration" class="selectpicker" data-style="select-with-transition" title="@MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.ChoosePeriod" data-size="7">
                     <option class="bs-title-option" value="">@MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.ChoosePeriod</option>
                     <option value="day"> @MOF.Etimad.Monafasat.Resources.TenderResources.DisplayInputs.Day</option>
                     <option value="month"> @MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.Month </option>
                     <option value="sixmonths">@MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.SixMonth</option>
                     <option value=""> @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.All</option>
                  </select>
               </div>
            </div>
            <div class="input-group-prepend pull-left etd-search-btn">
               <button type="button" class="btn btn-primary btn-block text-center btn-just-icon" id="supplierResultBtn" name="button" @@click="supplierResult">
                  <i class="material-icons">search</i>
               </button>
            </div>
         </div>
      </div>
      <div id="SuppliersInDurationDiv">
         <ul class="list-group form-details-list">
            <li class="list-group-item">
               <div class="row">
                  <div class="col-6 etd-item-title">
                     @MOF.Etimad.Monafasat.Resources.Dashboard.DisplayInputs.Numberofsuppliersregisteredinthisperiod
                  </div>
                  <div class="col-6 etd-item-info">
                     <span id="suppliersInDurationCount">10</span>
                  </div>
                  <div style="display:none">
                     <pagination :visible_pages="0" ref="vpaginator" :resource_url="resource_url" @@update="updateResource"></pagination>
                  </div>
               </div>
            </li>
         </ul>
      </div>
   </div>
</div>
