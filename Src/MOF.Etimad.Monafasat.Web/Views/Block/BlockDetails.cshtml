
@{
   ViewData["Title"] = "قوائم المنع";
   Layout = "~/Views/Shared/_Layout-ETD.cshtml";
}



<ul class="nav nav-pills nav-pills-icons" role="tablist">
   <li class="nav-item">
      <a class="nav-link text-center active show" href="#d-1" role="tab" data-toggle="tab" aria-selected="true">
         <i class="fa fa-user-times" aria-hidden="true"></i> 
         الموردين الممنوعين
      </a>
   </li>
   <li class="nav-item">
      <a class="nav-link text-center" id="add-new" href="#d-2" role="tab" data-toggle="tab" aria-selected="true">
         <i class="fa fa-user-plus" aria-hidden="true"></i> 
         أضافة مورد لقائمة لممنوعين
      </a>
   </li>
</ul>
<div class="tab-content  tab-space col-12 " style="padding:0">
   <div class="tab-pane active animated bounceInLeft  delay-1s show" id="d-1">
      <div class="card">
