<div class="animated  delay-2s col-md-12 col-sm-12 col-xs-12" id="CommitteeTable">
   <div class="row" v-if="Users && Users.length > 0">
      <div class="monafasausers-item monafasa-item col-xl-4 col-lg-4 col-md-6  col-sm-12 col-xs-12" v-for="(usr, $index) in Users">
         <div class="card">
            <div class="card-body">
               @*@Html.Partial("_UserListActions.cshtml")*@
               <h5 class="card-category card-category-social text-primary">

                  <i class="material-icons  text-dark-green" style="border: 1px solid #ccc;display: inline;border-radius: 100%;padding: 5px;">people</i> <span class="d-inline-block" style="padding: 0px 5px;">{{usr.commericalRegisterName}}</span>

               </h5>
               <div class="clearfix"></div>
               <div class="row">
                  <ul class="list-group form-details-list">
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              #
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>{{(currentPage -1) * (pageSize) +  $index + 1}}</span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.VendorsResources.DisplayInputs.CommericalRegisterNo
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>
                                 {{usr.commericalRegisterNo}}
                              </span>
                           </div>
                        </div>
                     </li>
                     <li class="list-group-item">
                        <div class="row">
                           <div class="col etd-item-title">
                              @MOF.Etimad.Monafasat.Resources.VendorsResources.DisplayInputs.Email
                           </div>
                           <div class="col-8 etd-item-info">
                              <span>
                                 {{usr.email}}
                              </span>
                           </div>
                        </div>
                     </li>
                  </ul>
               </div>
            </div>
            <div class="card-footer row">
               <div class="col p-0 text-right">
                  <button class="btn btn-sm btn-primary" type="button" @@click="getSupplierInfo(usr.commericalRegisterNo)">@MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.Details</button>
               </div>
            </div>
         </div>
      </div>
   </div>
   <div class="col-12" v-else>
      <div class="row text-center">
         <div class="card">
            <div class="card-body text-center">
               <i class="text-primary material-icons" style="font-size:3rem">report</i>
               <br />
               @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.NoData
            </div>
         </div>
      </div>
   </div>
   <div class="col-12 m-5 m-auto">
      <pagination :visible_pages="3" ref="vpaginator" :resource_url="resource_url" @@update="updateResource"></pagination>
   </div>
</div>
