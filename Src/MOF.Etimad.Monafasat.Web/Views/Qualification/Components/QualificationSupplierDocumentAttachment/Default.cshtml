@model List<MOF.Etimad.Monafasat.ViewModel.PreQualificationSupplierAttachmentModel>
<div class="row" id="TenderDetials">
   <div class="col-12">
      <h4 class="text-primary">وثائق التأهيل</h4>
   </div>
   <div class="col-md-6 col-sm-12 col-xs-12">
      <ul class="list-group form-details-list">
         <li class="list-group-item">
            <div class="row">
               <div class="col-4 etd-item-title">
                  وثائق التأهيل
               </div>
               @if (Model.Count > 0)
               {
                  <div class="col-8 etd-item-info">
                     @foreach (var item in Model)
                     {
                        <a href="#" onclick="RedirectURL('@item.FileNetReferenceId','@item.FileName'); return false;" target="_blank" class="btn btn-outline-primary btn-sm" style="white-space: normal;">
                           <i class="material-icons">insert_drive_file</i> @item.FileName
                        </a>
                     }
                  </div>
               }
               else
               {
                  <div class="col-8 etd-item-info">
                     <i class="text-primary material-icons" style="font-size:3rem">report</i> 
                     @MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.NoData
                  </div>
               }
            </div>
         </li>
      </ul>
   </div>
</div>
<script>
   function RedirectURL(ids, names) {
      window.open("/Upload/getfile/" + ids + ":" + names, '_blank');
   }
</script>
