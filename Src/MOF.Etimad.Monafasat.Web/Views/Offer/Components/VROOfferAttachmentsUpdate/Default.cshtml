@using MOF.Etimad.Monafasat.SharedKernel;
@using res = MOF.Etimad.Monafasat.Resources
@model MOF.Etimad.Monafasat.ViewModel.OfferDetailModel
<style>
   .form-check-input {
      position: relative;
      margin-top: .3rem;
      margin-left: 0.5rem;
   }
</style>
<div id="app">
   <input type="hidden" id="hdnisNewBank" value="1" />
   <input type="hidden" id="hdnisNewClassification" value="1" />
   <input type="hidden" name="OfferIdString" asp-for="OfferIdString" id="hdOfferId" />
   @Html.AntiForgeryToken()
   <div class="modal fade" id="myModal3" tabindex="-1" role="dialog">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">  @res.OfferResources.DisplayInputs.BankGuaranteeLetter</h5>
               <button type="button" class="close pull-right" data-dismiss="modal" aria-label="Close">
                  <i class="material-icons">clear</i> 
               </button>
            </div>
            <div class="modal-body">
               <div class="" id="divBankGuarantee">
                  <div class="row">
                     <div class="col-sm-6 col-xs-12">
                        <div class="form-group bmd-form-group">
                           <label class="label bmd-label-static"> @res.OfferResources.DisplayInputs.BankGuaranteeStatus</label>
                           <div class="form-check">
                              <label class="form-check-label">
                                 <input name="guaranteeStatus" class="form-check-input isFinancialChecking" checked type="radio" id="guaranteeStatus1" value="true">
                                 @res.OfferResources.DisplayInputs.Valid
                                 <span class="circle">
                                    <span class="check"></span>
                                 </span>
                              </label>
                              <label class="form-check-label">
                                 <input name="guaranteeStatus" class="form-check-input isFinancialChecking" type="radio" id="guaranteeStatus2" value="false">
                                 @res.OfferResources.DisplayInputs.Invalid
                                 <span class="circle">
                                    <span class="check"></span>
                                 </span>
                              </label>
                           </div>
                        </div>
                     </div>
                     <div class="col-sm-6 col-xs-12">
                        <div class="form-group bmd-form-group">
                           <label class="label bmd-label-static">@res.OfferResources.DisplayInputs.BankGuaranteeNumber</label><span class="text-danger">*</span>
                           <input class="form-control isFinancialChecking" name="guaranteeNumber" required id="tb_GuaranteeNUmber" />
                           <span id="vdGuaranteeNUmber" class="bmd-help text-danger">@res.OfferResources.ErrorMessages.EnterGuaranteeNumber </span>
                        </div>
                     </div>
                     <div class="col-sm-6 col-xs-12">
                        <div class="form-group bmd-form-group">
                           <label label class="label bmd-label-static"> @res.OfferResources.DisplayInputs.GuaranteeBankName</label><span class="text-danger">*</span>
                           <select asp-for="Bank" id="bankName" name="bankName" required data-live-search="true" title="@res.SharedResources.DisplayInputs.Select" class="selectpicker"
                                   asp-items="@(new SelectList(Model.Banks, "BankId", "Name"))" data-style="select-with-transition" data-size="7">
                              <option value="">@MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.Select</option>
                           </select>
                           <span id="vdbankName" class="bmd-help text-danger">@res.OfferResources.ErrorMessages.SelectBank</span>
                        </div>
                     </div>
                     <div class="  col-sm-6 col-xs-12">
                        <div class="form-group bmd-form-group">
                           <label class="label bmd-label-static"> @res.OfferResources.DisplayInputs.GuaranteeValue </label><span class="text-danger">*</span>
                           <input type="number" min="0" class="form-control isFinancialChecking" required id="tb_GuaranteeValue" value="">
                           <span id="vdGuaranteeValue" class="bmd-help text-danger">@res.OfferResources.ErrorMessages.EnterBankGuaranteeValue</span>
                        </div>
                     </div>
                     <div class="  col-sm-6 col-xs-12">
                        <div class="form-group bmd-form-group">
                           <label class="label bmd-label-static">@res.OfferResources.DisplayInputs.BankGuaranteeDays</label>
                           <input type="number" min="0" class="form-control isFinancialChecking" id="tb_GuaranteeDays" value="">
                        </div>
                     </div>
                     <div class="  col-sm-6 col-xs-12">
                        <div class="form-group form-check etd-calendar-group">
                           <label for="datepick" class="label bmd-label-static"> @res.OfferResources.DisplayInputs.GuaranteeStartDate</label>
                           <input type="text" class="form-control datepicker-mix datepicker-gregorian inputMask isFinancialChecking" id="tb_GuaranteeStartDate" name="guaranteeStartDate" placeholder="" value="">
                           <span id="vdDates" class="bmd-help text-danger">@res.OfferResources.ErrorMessages.StartDateGreaterThanEndDate</span>
                        </div>
                     </div>
                     <div class="  col-sm-6 col-xs-12">
                        <div class="form-group form-check etd-calendar-group">
                           <label for="datepick" class="label bmd-label-static">@res.OfferResources.DisplayInputs.GuaranteeEndDate</label>
                           <input type="text" class="form-control datepicker-mix datepicker-gregorian inputMask isFinancialChecking" id="tb_GuaranteeEndDate" name="guaranteeEndDate" placeholder="" value="">
                        </div>
                     </div>
                  </div>
               </div>
            </div>
            <div class="modal-footer">
               <button class="btn btn-primary btn-sm" type="button"
                       id="uploadGuaranteexxx" onclick="AddBankGuarantee()">
                  @res.SharedResources.DisplayInputs.Save
               </button>
               <button type="button" class="btn btn-danger btn-link" data-dismiss="modal">@res.SharedResources.DisplayInputs.Cancel</button>
            </div>
         </div>
      </div>
   </div>
   <div class="modal fade" id="myModal2" tabindex="-1" role="dialog">
      <div class="modal-dialog" role="document">
         <div class="modal-content">
            <div class="modal-header">
               <h5 class="modal-title">@res.OfferResources.DisplayInputs.ClassificationCertificate</h5>
               <button type="button" class="close pull-right" data-dismiss="modal" aria-label="Close">
                  <i class="material-icons">clear</i> 
               </button>
            </div>
            <div class="modal-body">
               <div class="row" id="divSpecification">
                  <div class="  col-sm-6 col-xs-12">
                     <div class="form-group bmd-form-group">
                        <label class="label bmd-label-static">@res.OfferResources.DisplayInputs.ClassificationCertificateFor</label>
                        <div class="form-check">
                           <label class="form-check-label">
                              <input name="isForApplier" class="form-check-input" checked type="radio" id="isForApplier1" value="true">
                              @res.OfferResources.DisplayInputs.OfferApplier
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                           <label class="form-check-label">
                              <input name="isForApplier" class="form-check-input" type="radio" id="isForApplier2" value="false">
                              @res.OfferResources.DisplayInputs.Guarantor
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                        </div>
                     </div>
                  </div>
                  <div class="col-12 "><hr /><h4 class="h5">@res.OfferResources.DisplayInputs.ClassificationField</h4></div>
                  <div class="col-sm-6 col-xs-12">
                     <div class="form-group bmd-form-group">
                        <label label class="label bmd-label-static">@res.OfferResources.DisplayInputs.ConstuctionWork</label>
                        <select asp-for="ConstructionWork" data-live-search="true" class="selectpicker" id="ConstructionWorklst"
                                asp-items="@(new SelectList(Model.ConstructionWorks, "ConstructionWorkId", "Name"))" data-style="select-with-transition" data-size="7">
                           <option value="">@MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.Select</option>
                        </select>
                     </div>
                  </div>
                  <div class="col-sm-6 col-xs-12">
                     <div class="form-group bmd-form-group">
                        <label label class="label bmd-label-static">@res.OfferResources.DisplayInputs.RunningMentainaceWork</label>
                        <select asp-for="MaintenanceRunningWork" class="selectpicker" id="MaintenanceWorklst"
                                asp-items="@(new SelectList(Model.MaintenanceRunningWorks, "MaintenanceRunningWorkId", "Name"))" data-style="select-with-transition" data-size="7">
                           <option value="">@MOF.Etimad.Monafasat.Resources.SharedResources.DisplayInputs.Select</option>
                        </select>
                     </div>
                  </div>
                  <div class="col-sm-6 col-xs-12">
                     <div class="form-group bmd-form-group">
                        <label label class="label bmd-label-static">@res.OfferResources.DisplayInputs.ClassificationDegree </label>
                        <select id="specificationDegree" data-live-search="true" class="selectpicker"
                                data-style="select-with-transition" title="@res.SharedResources.DisplayInputs.Select" data-size="7">
                           <option value="">@res.SharedResources.DisplayInputs.Select</option>
                           <option value="1">1</option>
                           <option value="2">2</option>
                           <option value="3">3</option>
                           <option value="4">4</option>
                           <option value="5">5</option>
                           <option value="6">6</option>
                        </select>
                     </div>
                  </div>
                  <div class="clearfix"></div>
               </div>
            </div>
            <div class="modal-footer">
               <button class="btn btn-primary btn-sm" type="button"
                       id="uploadClassificationxxx" onclick="AddSpecification()"
                       value="">
                  @res.SharedResources.DisplayInputs.Save
               </button>
               <button type="button" class="btn btn-danger btn-link" data-dismiss="modal">@res.SharedResources.DisplayInputs.Cancel</button>
            </div>
         </div>
      </div>
   </div>
   <div class="clearfix"></div>
   <div class="row">
      <div class="col-12">
         <div class="row tendar-navigation position-relative m-3" style="min-height: 0px;">
            <ul class="nav nav-pills nav-pills-teal col-12">
               <li class="nav-item active leader">
                  <a href="#pill1" data-toggle="tab" class="nav-link active">
                     <i class="material-icons pull-left" style="padding: 0px 3px;font-size: 24px;">contacts</i> 
                     <i class="material-icons pull-left text-warning" style="padding: 0px 3px;font-size: 24px;">star</i> @Model.SupplierName
                  </a>
               </li>
            </ul>
         </div>
      </div>
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
         <h5 class="text-dark-green">@res.OfferResources.DisplayInputs.BankGuarantee</h5>
         <div class="card">
            <div class="col-sm-6 col-xs-12">
               <div class="form-group bmd-form-group">
                  <label id="" class="label bmd-label-static">@res.OfferResources.DisplayInputs.Attached </label><span class="text-danger">*</span>
                  <div class="form-check">
                     <label class="form-check-label">
                        <input name="IsBankGuaranteeAttached" class="form-check-input isFinancialChecking" checked type="radio" id="IsBankGuaranteeAttached1" value="true">
                        @res.SharedResources.DisplayInputs.Yes
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                     <label class="form-check-label">
                        <input asp-for="IsBankGuaranteeAttached" name="IsBankGuaranteeAttached" class="form-check-input isFinancialChecking" type="radio" id="IsBankGuaranteeAttached2" value="false">
                        @res.SharedResources.DisplayInputs.No
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                  </div>
               </div>
            </div>
            <table class="table">
               <caption></caption>
               <thead style="background-color: #e0efe8;">
                  <tr>
                     <th scope="col">@res.OfferResources.DisplayInputs.BankGuaranteeStatus</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.BankGuaranteeNumber</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.GuaranteeValue</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.BankGuaranteeDays</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.GuaranteeBankName</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.GuaranteeStartDate</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.GuaranteeEndDate</th>
                     @{
                        if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                        {
                           <th scope="col"></th>
                        }
                     }
                  </tr>
               </thead>
               <tbody>
                  <tr class="GuaranteesGrid" v-for="(tbl,indexD) in BankGuarantees">
                     <td data-label=" Milestone">
                        <span> {{tbl.isBankGuaranteeValidString}}</span>
                        <span style="display:none"> {{tbl.isBankGuaranteeValid}}</span>
                     </td>
                     <td data-label=" Quantity">
                        <span> {{tbl.guaranteeNumber}}</span>
                     </td>
                     <td data-label="Unit">
                        <span> {{tbl.amount}}</span>
                     </td>
                     <td data-label="Details">
                        <span> {{tbl.guaranteeDays}}</span>
                     </td>
                     <td data-label="Details">
                        <span> {{tbl.bankName}}</span>
                        <span style="display:none"> {{tbl.BankId}}</span>
                     </td>
                     <td data-label="Details">
                        <span format="MM-dd-yyyy"> {{tbl.guaranteeStartDateDisplay}}</span>
                     </td>
                     <td data-label="Details">
                        <span format="MM-dd-yyyy"> {{tbl.guaranteeEndDateDisplay}}</span>
                     </td>
                     @{ if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                         {
                        <td>
                           <div class="btn-group" role="group">
                              <button type="button" class="btn btn-just-icon btn-danger btn-link btn-sm" data-toggle="tooltip" data-placement="top" @@click="deleteGuarantee(indexD)">
                                 <i class="material-icons">delete</i>
                              </button>
                              <button type="button" class="btn btn-just-icon btn-warning btn-link btn-sm" data-toggle="modal" data-placement="top" data-target="#myModal3" @@click="editGuarantee(indexD)">
                                 <i class="material-icons">edit</i> 
                              </button>
                           </div>
                        </td>
                        } }
                  </tr>
               </tbody>
               <tfoot>
                  @{
                     if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                     {
                        <tr>
                           <td colspan="9">
                              <button type="button" class="btn btn-primary btn-sm pull-right" name="button" @@click="AddNewGuarantee" data-toggle="modal" data-target="#myModal3">@res.OfferResources.DisplayInputs.AddBankGuarantee</button>
                           </td>
                        </tr>
                     }
                  }
               </tfoot>
            </table>
         </div>
      </div>
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
         <h5 class="text-dark-green">@res.OfferResources.DisplayInputs.ClassificationCertificate</h5>
         <div class="card">
            <div class="col-sm-6 col-xs-12">
               <div class="form-group bmd-form-group">
                  <label id="" class="label bmd-label-static">@res.OfferResources.DisplayInputs.Attached </label><span class="text-danger">*</span>
                  <div class="form-check">
                     <label class="form-check-label">
                        <input asp-for="IsSpecificationAttached" name="IsSpecificationAttached" class="form-check-input" checked type="radio" id="IsSpecificationAttached1" value="true">
                        @res.SharedResources.DisplayInputs.Yes
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                     <label class="form-check-label">
                        <input asp-for="IsSpecificationAttached" name="IsSpecificationAttached" class="form-check-input" type="radio" id="IsSpecificationAttached2" value="false">
                        @res.SharedResources.DisplayInputs.No
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                  </div>
               </div>
            </div>
            <div class="  col-sm-6 col-xs-12">
               <div class="form-group bmd-form-group">
                  <label class="label bmd-label-static">@res.OfferResources.DisplayInputs.ClassificationCertificateStatus  </label>
                  <div class="form-check">
                     <label class="form-check-label">
                        <input asp-for="IsSpecificationValidDate" name="IsSpecificationValidDate" class="form-check-input" checked type="radio" id="IsSpecificationValidDate1" value="true">
                        @res.SharedResources.DisplayInputs.Valid
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                     <label class="form-check-label">
                        <input asp-for="IsSpecificationValidDate" name="IsSpecificationValidDate" class="form-check-input" type="radio" id="IsSpecificationValidDate2" value="false">
                        @res.SharedResources.DisplayInputs.Invalid
                        <span class="circle">
                           <span class="check"></span>
                        </span>
                     </label>
                  </div>
               </div>
            </div>
            <table class="table">
               <caption></caption>
               <thead style="background-color: #e0efe8;">
                  <tr>
                     <th scope="col">@res.OfferResources.DisplayInputs.ClassificationCertificateFor</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.ClassificationDegree </th>
                     <th scope="col">@res.OfferResources.DisplayInputs.ConstuctionWork</th>
                     <th scope="col">@res.OfferResources.DisplayInputs.RunningMentainaceWork</th>
                     @{if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                         {
                           <th scope="col"></th>
                         }}
                  </tr>
               </thead>
               <tbody>
                  <tr class="SpecificationsGrid" v-for="(tbl,indexD) in Specifications">
                     <td data-label=" Quantity">
                        <span> {{tbl.isForApplierString}}</span>
                        <span style="display:none"> {{tbl.iIsForApplier}}</span>
                     </td>
                     <td data-label="Unit">
                        <span> {{tbl.degree}}</span>
                     </td>
                     <td data-label="Details">
                        <span> {{tbl.constructionWorkString}}</span>
                        <span style="display:none"> {{tbl.constructionWorkId}}</span>
                     </td>
                     <td data-label="Details">
                        <span> {{tbl.maintenanceRunningWorkString}}</span>
                        <span style="display:none"> {{tbl.maintenanceRunningWorkId}}</span>
                     </td>
                     @{if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                        {
                           <td>
                              <div class="btn-group" role="group">
                                 <button type="button" class="btn btn-just-icon btn-danger btn-link btn-sm" data-toggle="tooltip" data-placement="top" @@click="deleteSpecification(indexD)">
                                    <i class="material-icons">delete</i> 
                                 </button>
                                 <button type="button" class="btn btn-just-icon btn-warning btn-link btn-sm" data-placement="top" @@click="editSpecification(indexD)" id="btnEditSpecification" data-toggle="modal" data-target="#myModal2">
                                    <i class="material-icons">edit</i> 
                                 </button>
                              </div>
                           </td>
                        }}
                  </tr>
               </tbody>
               <tfoot>
                  @{
                     if (!User.IsInRole(RoleNames.OffersOpeningAndCheckManager) && !(User.IsInRole(RoleNames.OffersOpeningAndCheckSecretary) && Model.TenderStatusId == (int)Enums.TenderStatus.VROOffersFinancialCheckingPending))
                     {
                        <tr>
                           <td colspan="7">
                              <button type="button" class="btn btn-primary btn-sm pull-right" name="button" id="btnClassModal" data-toggle="modal" @@click="AddNewSpecification" data-target="#myModal2">@res.OfferResources.DisplayInputs.AddClassification</button>
                           </td>
                        </tr>
                     }
                  }
               </tfoot>
            </table>
         </div>
      </div>
      @if (Model.CombinedOwner == true)
      {
         <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
            <h5 class="text-dark-green"> @res.OfferResources.DisplayInputs.PurchaseReceipt</h5>
            <div class="card">
               <div class="card-body">
                  <div class=" ">
                     <div class="form-group bmd-form-group">
                        <span id="" class="label bmd-label-static">@res.OfferResources.DisplayInputs.Attached</span><span class="text-danger">*</span>
                        <div class="form-check">
                           <label class="form-check-label">
                              <input name="IsPurchaseBillAttached" class="form-check-input" checked type="radio" id="isBillAttached1" value="true" @(User.IsInRole(RoleNames.OffersOpeningAndCheckManager) ? "disabled='disabled'" : "" )>
                              @res.SharedResources.DisplayInputs.Yes
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                           <label class="form-check-label">
                              <input asp-for="IsPurchaseBillAttached" name="IsPurchaseBillAttached" class="form-check-input" type="radio" id="isBillAttached2" value="false">
                              @res.SharedResources.DisplayInputs.No
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
            <h5 class="text-dark-green">@res.OfferResources.DisplayInputs.SiteVisitation</h5>
            <div class="card">
               <div class="card-body">
                  <div class=" ">
                     <div class="form-group bmd-form-group">
                        <label id="" class="label bmd-label-static">@res.OfferResources.DisplayInputs.Attached</label><span class="text-danger">*</span>
                        <div class="form-check">
                           <label class="form-check-label">
                              <input asp-for="IsVisitationAttached" name="IsVisitationAttached" class="form-check-input" checked type="radio" id="isVisitationFileAttached1" value="true">
                              @res.SharedResources.DisplayInputs.Yes
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                           <label class="form-check-label">
                              <input asp-for="IsVisitationAttached" name="IsVisitationAttached" class="form-check-input" type="radio" id="isVisitationFileAttached2" value="false">
                              @res.SharedResources.DisplayInputs.No
                              <span class="circle">
                                 <span class="check"></span>
                              </span>
                           </label>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      }
      <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
         <h5 class="text-dark-green">@res.OfferResources.DisplayInputs.ZakatCerificate</h5>
         <div class="card">
            <div class="card-body">
               <div class=" ">
                  <div class="form-group bmd-form-group">
                     <label id="" class="label bmd-label-static">@res.OfferResources.DisplayInputs.Attached </label><span class="text-danger">*</span>
                     <div class="form-check">
                        <label class="form-check-label">
                           <input asp-for="IsZakatAttached" name="IsZakatAttached" class="form-check-input" checked type="radio" id="IsZakatAttached1" value="true">
                           @res.SharedResources.DisplayInputs.Yes
                           <span class="circle">
                              <span class="check"></span>
                           </span>
                        </label>
                        <label class="form-check-label">
                           <input asp-for="IsZakatAttached" name="IsZakatAttached" class="form-check-input" type="radio" id="IsZakatAttached2" value="false">
                           @res.SharedResources.DisplayInputs.No
                           <span class="circle">
                              <span class="check"></span>
                           </span>
                        </label>
                     </div>
                  </div>
               </div>
               <div>
                  <div class="form-group bmd-form-group">
                     <label class="label bmd-label-static">@res.OfferResources.DisplayInputs.ZakatCerificateStatus</label>
                     <div class="form-check">
